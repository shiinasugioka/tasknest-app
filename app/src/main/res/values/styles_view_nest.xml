<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="view_nest_button">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">35dp</item>
        <item name="android:background">?android:attr/selectableItemBackground</item>
        <item name="android:fontFamily">@font/poppins</item>
        <item name="android:paddingTop">0dp</item>
        <item name="android:paddingBottom">0dp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textColor">@color/primary_text</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="underline_bar_view_nest">
        <item name="android:layout_width">80dp</item>
        <item name="android:layout_height">3dp</item>
        <item name="android:background">@drawable/underline_rectangle</item>
    </style>

    <style name="SpinnerTextViewItem" parent="@android:style/Widget.TextView">
        <item name="android:textSize">20sp</item>
        <item name="android:fontFamily">@font/poppins</item>
        <item name="android:paddingStart">10dp</item>
        <item name="android:paddingEnd">10dp</item>
    </style>

    <style name="nest_drop_down">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">1dp</item>
        <item name="android:layout_marginEnd">210dp</item>
        <item name="android:layout_marginBottom">470dp</item>
    </style>

</resources>